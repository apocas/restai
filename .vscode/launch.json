{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Python: Main File",
      "type": "debugpy",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [],
      "purpose": [
        "debug-in-terminal"
      ],
      "env": {
        "RESTAI_DEV": "true",
        "LD_LIBRARY_PATH": "${env:LD_LIBRARY_PATH}:${workspaceFolder}/.venvs/.venv-whisperx/lib/python3.12/site-packages/nvidia/cudnn/lib"
      }
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${fileDirname}",
      "args": [],
      "purpose": [
        "debug-in-terminal"
      ],
      "env": {
        "LD_LIBRARY_PATH": "${env:LD_LIBRARY_PATH}:${workspaceFolder}/.venvs/.venv-whisperx/lib/python3.12/site-packages/nvidia/cudnn/lib"
      }
    }
  ]
}